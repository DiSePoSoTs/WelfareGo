	<queryString>
		<![CDATA[select row_number() over(order by ac.cognome) as prog, ac.cognome,ac.nome, ac.cod_fisc, pi.dt_avvio,
add_months(pi.dt_avvio,pi.dur_mesi) as scadenza,
coalesce(sum(pm.bdg_prev_eur),0), coalesce(sum(pm.bdg_cons_eur),0), coalesce(max(pm.cod_imp),'0'), pe.id_evento,
pd.des_param as uot,ti.des_tipint as titolo,pd1.des_param as fascia ,pd2.des_param as misura

from anagrafe_soc ac join pai pa on (ac.cod_ana = pa.cod_ana) join
pai_intervento pi on (pa.cod_pai=pi.cod_pai) join
pai_intervento_mese pm on (pi.cod_pai=pm.cod_pai and pi.cod_tipint=pm.cod_tipint and pi.cnt_tipint=pm.cnt_tipint) join
pai_evento pe on (pi.cod_pai=pe.cod_pai and pi.cod_tipint=pe.cod_tipint and pi.cnt_tipint=pe.cnt_tipint)  join
parametri_indata pd on pa.id_param_uot = pd.id_param_indata  join
parametri_indata pd1 on pa.id_param_fascia = pd1.id_param_indata join
tipologia_intervento ti on pi.cod_tipint=ti.cod_tipint join
parametri_indata pd2 on ti.id_param_uni_mis = pd2.id_param_indata

where $X{IN,pe.id_evento,lista_eventi}

group by ac.cognome, ac.nome, ac.cod_fisc, pi.dt_avvio, pi.dur_mesi, pe.id_evento,
pd.des_param,ti.des_tipint,pd1.des_param,pd2.des_param
order by 2]]>
	</queryString>
